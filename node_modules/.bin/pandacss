#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/m1l73r/Workspace/codra-default-webui/node_modules/.pnpm/@pandacss+dev@0.53.7_typescript@5.8.3/node_modules/@pandacss/dev/node_modules:/home/m1l73r/Workspace/codra-default-webui/node_modules/.pnpm/@pandacss+dev@0.53.7_typescript@5.8.3/node_modules/@pandacss/node_modules:/home/m1l73r/Workspace/codra-default-webui/node_modules/.pnpm/@pandacss+dev@0.53.7_typescript@5.8.3/node_modules:/home/m1l73r/Workspace/codra-default-webui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/m1l73r/Workspace/codra-default-webui/node_modules/.pnpm/@pandacss+dev@0.53.7_typescript@5.8.3/node_modules/@pandacss/dev/node_modules:/home/m1l73r/Workspace/codra-default-webui/node_modules/.pnpm/@pandacss+dev@0.53.7_typescript@5.8.3/node_modules/@pandacss/node_modules:/home/m1l73r/Workspace/codra-default-webui/node_modules/.pnpm/@pandacss+dev@0.53.7_typescript@5.8.3/node_modules:/home/m1l73r/Workspace/codra-default-webui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@pandacss+dev@0.53.7_typescript@5.8.3/node_modules/@pandacss/dev/bin.js" "$@"
else
  exec node  "$basedir/../.pnpm/@pandacss+dev@0.53.7_typescript@5.8.3/node_modules/@pandacss/dev/bin.js" "$@"
fi
